apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: xenforo
  annotations:
    kubernetes.io/ingress.class: nginx

    # maxxx
    nginx.ingress.kubernetes.io/proxy-body-size: 50m
    nginx.ingress.kubernetes.io/proxy-read-timeout: '600'
spec:
  rules:
  - host: <%= _ingressHost %>
    http:
      paths:
      - path: /data
        backend:
          serviceName: ftp
          servicePort: 80
      - backend:
          serviceName: xenforo
          servicePort: 80
